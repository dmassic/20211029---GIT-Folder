NOP
'*****************************
'READ THE LEFT TWO DIGITS OF
'TWS AND SET IN B010.
'*****************************
DOUT OT#(17) OFF
DOUT OT#(18) OFF
DOUT OT#(19) ON
DOUT OT#(20) OFF
TIMER T=0.20
DIN B010 IGH#(5)
TIMER T=0.10
DOUT OT#(19) OFF
DOUT OT#(20) ON
TIMER T=0.20
DIN B011 IGH#(5)
TIMER T=0.10
DOUT OT#(20) OFF
MUL B011 10
ADD B010 B011
END
